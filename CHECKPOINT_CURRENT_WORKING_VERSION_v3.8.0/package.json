{
  "name": "aws-opportunity-analysis",
  "version": "1.0.0",
  "description": "AWS Opportunity Analysis Application",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "debug": "node app-debug.js",
    "frontend": "node frontend-server.js",
    "dev-frontend": "nodemon frontend-server.js",
    "dev-all": "concurrently \"npm run dev\" \"npm run dev-frontend\"",
    "start:debug": "node app-debug.js",
    "dev:debug": "nodemon app-debug.js",
    "dev-all:debug": "concurrently \"npm run dev:debug\" \"npm run dev-frontend\"",
    "validate:aws": "node scripts/validate-aws-connectivity.js",
    "validate:bedrock": "node scripts/validate-bedrock-connectivity.js",
    "validate:lambda": "node scripts/validate-lambda-functions.js",
    "validate:infrastructure": "node scripts/validate-infrastructure.js",
    "validate:security": "node scripts/validate-security.js",
    "test:connectivity": "node scripts/run-connectivity-tests.js",
    "test:connectivity:all": "node scripts/run-connectivity-tests.js full bedrock lambda infrastructure",
    "validate:all": "npm run test:connectivity:all && npm run validate:security",
    "validate:production": "node scripts/validate-production-readiness.js",
    "test:production": "node scripts/test-production-startup.js",
    "test:validation-tools": "node scripts/test-validation-tools.js",
    "deploy:infrastructure": "node scripts/deploy-infrastructure.js",
    "deploy:task-2": "node scripts/deploy-task-2.js",
    "deploy:lambda": "node scripts/deploy-lambda-functions.js",
    "deploy:dynamodb": "node scripts/deploy-dynamodb.js",
    "deploy:eventbridge": "node scripts/deploy-eventbridge.js",
    "test:prerequisites": "node scripts/test-deployment-prerequisites.js",
    "test:error-handling": "node scripts/test-error-handling-monitoring.js",
    "validate:task-6": "node scripts/validate-task-6-implementation.js",
    "validate:task-6-simple": "node scripts/simple-task-6-validation.js",
    "integrate:error-handling": "node scripts/integrate-error-handling.js",
    "test:framework": "node scripts/validate-testing-framework.js",
    "test:comprehensive": "node scripts/run-comprehensive-testing-framework.js",
    "test:comprehensive:all": "node scripts/run-comprehensive-testing-framework.js --all",
    "test:comprehensive:quick": "node scripts/run-comprehensive-testing-framework.js --quick",
    "test:health": "node scripts/run-comprehensive-testing-framework.js --health-check --no-scenarios --no-error-scenarios --no-validation --no-diagnostics",
    "test:scenarios": "node scripts/run-comprehensive-testing-framework.js --scenarios --no-health-check --no-error-scenarios --no-validation --no-diagnostics",
    "test:performance": "node scripts/run-comprehensive-testing-framework.js --performance --no-health-check --no-scenarios --no-error-scenarios --no-validation --no-diagnostics",
    "test:diagnostics": "node scripts/run-comprehensive-testing-framework.js --diagnostics --no-health-check --no-scenarios --no-error-scenarios --no-validation"
  },
  "dependencies": {
    "@aws-sdk/client-athena": "^3.0.0",
    "@aws-sdk/client-bedrock-agent": "^3.0.0",
    "@aws-sdk/client-bedrock-agent-runtime": "^3.0.0",
    "@aws-sdk/client-bedrock-runtime": "^3.0.0",
    "@aws-sdk/client-cloudwatch": "^3.841.0",
    "@aws-sdk/client-cloudwatch-logs": "^3.840.0",
    "@aws-sdk/client-dynamodb": "^3.840.0",
    "@aws-sdk/client-eventbridge": "^3.840.0",
    "@aws-sdk/client-iam": "^3.840.0",
    "@aws-sdk/client-lambda": "^3.0.0",
    "@aws-sdk/client-s3": "^3.842.0",
    "@aws-sdk/client-secrets-manager": "^3.840.0",
    "@aws-sdk/client-sns": "^3.840.0",
    "@aws-sdk/client-sqs": "^3.840.0",
    "@aws-sdk/client-ssm": "^3.840.0",
    "@aws-sdk/client-sts": "^3.840.0",
    "@aws-sdk/lib-dynamodb": "^3.840.0",
    "archiver": "^5.3.1",
    "aws-sdk": "^2.1691.0",
    "axios": "^1.10.0",
    "body-parser": "^1.20.0",
    "dotenv": "^16.0.0",
    "express": "^4.18.0",
    "http-proxy-middleware": "^2.0.0",
    "ioredis": "^5.6.1",
    "jszip": "^3.10.1",
    "node-fetch": "^2.7.0"
  },
  "devDependencies": {
    "concurrently": "^8.0.0",
    "nodemon": "^3.0.0"
  }
}
